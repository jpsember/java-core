<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">

  <dict>

    <key>Label</key>
      <string>com.user.loginscript</string>

    <key>ProgramArguments</key>
      <array>
        <string>java</string>
        <string>-jar</string>
        <string>/Users/home/screenshotter.jar</string>
      </array>

    <key>KeepAlive</key>
      <true/>

    <key>StandardOutPath</key>
    <string>/Users/home/Desktop/screenshotter_logfile.txt</string>
    <key>StandardErrorPath</key>
    <string>/Users/home/Desktop/screenshotter_logfile_err.txt</string>

  </dict>

</plist>
