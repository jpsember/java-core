WS:  $_WHITESPACE  | $_JAVACOMMENT | $_HASHCOMMENT

BROP: \[
BRCL: \]
TRUE: true
FALSE: false
NULL: null

CBROP: \{
CBRCL: \}
COMMA: \,
COLON: :,
STRING: {_STRING}
#  (Not used) SINGLESTRING: {_SINGLESTRING}

# For numbers, see https://www.json.org/img/number.png

_NUMINTEGER: ( \-? (0 | ([1-9] \d*)) )
_NUMFRAC: (\. \d+ )
_EXP: ([eE] (\- | \+)? \d+)

NUMBER: {_NUMINTEGER} {_NUMFRAC}? {_EXP}?

